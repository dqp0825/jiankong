{% load staticfiles %}
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	    <link href="{% static 'vendors/bootstrap/dist/css/bootstrap.min.css' %}" rel="stylesheet">
	    <!-- Font Awesome -->
	    <link href="{% static 'vendors/font-awesome/css/font-awesome.min.css'%}" rel="stylesheet">
	    <!-- NProgress -->
	    <link href="{% static 'vendors/nprogress/nprogress.css'%}" rel="stylesheet">
	    <!-- bootstrap-daterangepicker -->
	    <link href="{% static 'vendors/bootstrap-daterangepicker/daterangepicker.css'%}" rel="stylesheet">
	
	    <!-- Custom Theme Style -->
	    <link href="{% static 'build/css/custom.css'%}" rel="stylesheet">
	    <link rel="stylesheet" href="{% static 'production/plugins/layui-v2.2.5/layui/css/layui.css'%}"  media="all">
	    <!--<link rel="stylesheet" href="plugins/calendar/laydate.css" />-->
	    <link rel="stylesheet" href="{% static 'production/plugins/calendar/laydate.css'%}" />
	</head>
	<body>
		<div class="container" style="overflow: hidden;">
			
			<div class="title_left">
    	<h3 style="margin: 10px auto;">&nbsp;&nbsp;<small>自动发现</small></h3>
	</div>
	<div class="row">
		<div class="col-md-10 col-ls-10 col-xs-10 col-md-offset-1">
				  
			      <div class="row"style="margin: 10px 0" >
			      	<div class="col-md-2 col-xs-2 col-ls-2">
			      		<h5 style=" height: 32px; line-height: 32px;">按照IP自动发现:</h5>
			      	</div>
			      	<div class="col-md-9 col-xs-9 col-ls-9" >
			      		<div class="row">
			      			<div class="col-md-3 col-xs-3 col-ls-3"><input type="password" name="password" lay-verify="pass" placeholder="设备IP" autocomplete="off" class="layui-input" style="border: 1px solid #C6C6C6; height: 32px;"></div>
			      			<div class="col-md-3 col-xs-3 col-ls-3"><input type="password" name="password" lay-verify="pass" placeholder="开始IP" autocomplete="off" class="layui-input" style="border: 1px solid #C6C6C6; height: 32px;"></div>
			      			<div class="col-md-3 col-xs-3 col-ls-3"><input type="password" name="password" lay-verify="pass" placeholder="终止IP" autocomplete="off" class="layui-input" style="border: 1px solid #C6C6C6; height: 32px;"></div>  			
			      		</div>
					
		      		</div>
		      		<div class="col-md-1 col-xs-1 col-ls-1">
		      			<button type="submit" class="btn btn-success" style="float: right;  display: inline-block;">发现</button>
		      		</div>
			      </div>
			      <div class="row" style="margin: 10px 0">
			      	<div class="col-md-2 col-xs-2 col-ls-2">
			      		<h5 style="height: 32px; line-height: 32px;">按照时间自动发现:</h5>
			      	</div>
			      	<div class="col-md-3 col-sm-3 col-xs-3" >                        			
						<input placeholder="开始时间" class="laydate-icon" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})" >									                               							
			      	</div>
			      	<div class="col-md-3 col-sm-3 col-xs-3">                        			
						<input placeholder="结束时间" class="laydate-icon" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})">									                               							
			      	</div>
			      	<div class="col-md-3 col-sm-3 col-xs-3">  
			      		<select name="city" lay-verify="" style="width: 150px; height: 32px;">
					        <option value="">每天03:00</option>
					        <option value="">每周六03:00</option>
					        <option value="">每月15日03:00</option>
						</select>    
			      	</div>
			      	<div class="col-md-1 col-xs-1 col-ls-1">
		      			<button type="submit" class="btn btn-success" style="float: right;  display: inline-block;">发现</button>
		      		</div>
			      </div>
			       
				  <div style="height: 40px; line-height:40px;width:100%; margin:0  auto;background:#E0FFFF;padding-left: 20px; margin-top: 20px;">
	    				<h5 style="line-height: 40px; display: inline-block;">自动发现</h5>
	    			    <button type="submit" class="btn btn-success" style="float: right;  display: inline-block; margin-top: 3px;" id="btn-Install">安装</button>
	    		  </div>
				<table class="layui-hide" id="test"></table>
				</div>
	</div>
		</div>
	

       
          
